<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xliu.mapper.OrderMapper">

    <insert id="addOrder" parameterType="Orders">
        insert into orders (orderId,bookId,userId,borrowDate,returnDate)
        where values (#{orderId},#{bookId},#{userId},#{borrowDate},#{returnDate});
    </insert>
    
</mapper>